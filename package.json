        "type": "github"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/cheerio": "^0.22.35",
        "@types/fluent-ffmpeg": "^2.1.26",
        "@types/jest": "^29.5.12",
        "@types/module-alias": "^2.0.4",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "tslib": "^2.7.0"
