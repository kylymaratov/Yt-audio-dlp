{
    "name": "yt-audio-dlp",
    "version": "1.0.3",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nodemon --watch src --exec ts-node src/index.ts",
        "build": "npx tsc",
        "test": "jest --coverage",
        "lint": "prettier --check src",
        "format": "prettier --write src"
    },
    "husky": {
