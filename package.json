{
    "name": "audio-downloader",
    "version": "1.1.7",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "nodemon --watch src src/index.ts",
        "build": "npx tsc -p . && npx rollup -c",
        "test": "jest",
        "lint": "prettier --check src",
        "format": "prettier --write src"
    },
    "husky": {
        "hooks": {
            "pre-commit": "prettier --write src && git add -A ."
        }
    },
    "keywords": [
        "audio",
        "downloader",
        "music"
    ],
    "author": "KLM",
    "license": "MIT",
    "description": "Node.js library for downloading songs from popular music platforms",
    "dependencies": {
        "axios": "^1.7.7",
        "cheerio": "^1.0.0",
        "fluent-ffmpeg": "^2.1.3",
        "module-alias": "^2.2.3",
        "socks-proxy-agent": "^8.0.4"
    },
    "repository": {
        "url": "https://github.com/kylymaratov/ytdl-audio.git",
        "type": "github"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/axios": "^0.14.0",
        "@types/cheerio": "^0.22.35",
        "@types/fluent-ffmpeg": "^2.1.26",
        "@types/jest": "^29.5.12",
        "@types/module-alias": "^2.0.4",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "rollup": "^4.21.0",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "tslib": "^2.7.0"
    }
}
